{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "materialmetaSchema.json",
  "type": "object",
  "default": {},
  "title": "MaterialMeta Schema",
  "required": [
    "filename",
    "orgId",
    "size",
    "type",
    "serialVersionUID",
    "id",
    "createdAt",
    "createdBy",
    "modifiedBy"
  ],
  "properties": {
    "filename": {
      "type": "string",
      "default": "",
      "title": "Filename - identifies the file name in the material meta",
      "examples": [
        ""
      ]
    },
    "orgId": {
      "type": "string",
      "default": "",
      "title": "orgId - identifies the organization",
      "examples": [
        ""
      ]
    },
    "size": {
      "type": "integer",
      "default": 0,
      "title": "Size - identifies the size",
      "examples": [
        0
      ]
    },
    "type": {
      "type": "array",
      "default": [],
      "title": "Type - identifies the type of materialMeta",
      "items": {
        "type": "string",
        "examples": [
          "COMMON",
          "LOGO",
          "FAVICON"
        ]
      },
      "examples": [
        ["COMMON",
          "LOGO",
          "FAVICON"
        ]
      ]
    },
    "serialVersionUID": {
      "type": "integer",
      "default": 0,
      "title": "serialVersionUID - The Identifier serialVersionUID (internal)",
      "examples": [
        0
      ]
    },
    "id": {
      "type": "string",
      "default": "",
      "title": "Id - An Identifier(internal)",
      "examples": [
        ""
      ]
    },
    "createdAt": {
      "type": "object",
      "default": {},
      "title": "CreatedAt - The date when the creation was made(internal)",
      "required": [
        "MIN_SECOND",
        "MAX_SECOND",
        "serialVersionUID",
        "seconds",
        "nanos"
      ],
      "properties": {
        "MIN_SECOND": {
          "type": "integer",
          "default": 0,
          "title": "MIN_SECOND - The min_second Identifier(internal)",
          "examples": [
            0
          ]
        },
        "MAX_SECOND": {
          "type": "integer",
          "default": 0,
          "title": "MAX_SECOND - The max_second Identifier(internal)",
          "examples": [
            0
          ]
        },
        "serialVersionUID": {
          "type": "integer",
          "default": 0,
          "title": "serialVersionUID - The Identifier serialVersionUID (internal)",
          "examples": [
            0
          ]
        },
        "seconds": {
          "type": "integer",
          "default": 0,
          "title": "Seconds - The seconds details(internal)",
          "examples": [
            0
          ]
        },
        "nanos": {
          "type": "integer",
          "default": 0,
          "title": "Nanos - The nanos details(internal)",
          "examples": [
            0
          ]
        }
      },
      "examples": [{
        "MIN_SECOND": 0,
        "MAX_SECOND": 0,
        "serialVersionUID": 0,
        "seconds": 0,
        "nanos": 0
      }]
    },
    "createdBy": {
      "type": "string",
      "default": "",
      "title": "CreatedBy - The details of who made the creation(internal)",
      "examples": [
        ""
      ]
    },
    "modifiedBy": {
      "type": "string",
      "default": "",
      "title": "ModifiedBy - Identifies who did a modification(internal)",
      "examples": [
        ""
      ]
    }
  },
  "examples": [{
    "filename": "",
    "orgId": "",
    "size": 0,
    "type": [
      "COMMON",
      "LOGO",
      "FAVICON"
    ],
    "serialVersionUID": 0,
    "id": "",
    "createdAt": {
      "MIN_SECOND": 0,
      "MAX_SECOND": 0,
      "serialVersionUID": 0,
      "seconds": 0,
      "nanos": 0
    },
    "createdBy": "",
    "modifiedBy": ""
  }]
}