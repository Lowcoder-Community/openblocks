{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "templateSchema.json",
  "type": "object",
  "default": {},
  "title": "Template Schema",
  "required": [
    "name",
    "applicationId",
    "serialVersionUID",
    "id",
    "createdAt",
    "createdBy",
    "modifiedBy"
  ],
  "properties": {
    "name": {
      "type": "string",
      "default": "",
      "title": "Name - Identifies the template name",
      "examples": [
        ""
      ]
    },
    "applicationId": {
      "type": "string",
      "default": "",
      "title": "ApplicationId - identifies the application",
      "examples": [
        ""
      ]
    },
    "serialVersionUID": {
      "type": "integer",
      "default": 0,
      "title": "SerialVersionUID - The Identifier serialVersionUID (internal)",
      "examples": [
        0
      ]
    },
    "id": {
      "type": "string",
      "default": "",
      "title": "Id - An Identifier(internal)",
      "examples": [
        ""
      ]
    },
    "createdAt": {
      "type": "object",
      "default": {},
      "title": "CreatedAt - The date when the creation was made(internal)",
      "required": [
        "MIN_SECOND",
        "MAX_SECOND",
        "serialVersionUID",
        "seconds",
        "nanos"
      ],
      "properties": {
        "MIN_SECOND": {
          "type": "integer",
          "default": 0,
          "title": "MIN_SECOND - The min_second Identifier(internal)",
          "examples": [
            0
          ]
        },
        "MAX_SECOND": {
          "type": "integer",
          "default": 0,
          "title": "MAX_SECOND - The max_second Identifier(internal)",
          "examples": [
            0
          ]
        },
        "serialVersionUID": {
          "type": "integer",
          "default": 0,
          "title": "SerialVersionUID - The serialVersionUID details (internal)",
          "examples": [
            0
          ]
        },
        "seconds": {
          "type": "integer",
          "default": 0,
          "title":  "Seconds - The seconds details(internal)",
          "examples": [
            0
          ]
        },
        "nanos": {
          "type": "integer",
          "default": 0,
          "title": "Nanos - The nanos details(internal)",
          "examples": [
            0
          ]
        }
      },
      "examples": [{
        "MIN_SECOND": 0,
        "MAX_SECOND": 0,
        "serialVersionUID": 0,
        "seconds": 0,
        "nanos": 0
      }]
    },
    "createdBy": {
      "type": "string",
      "default": "",
      "title": "CreatedBy - The details of who made the creation(internal)",
      "examples": [
        ""
      ]
    },
    "modifiedBy": {
      "type": "string",
      "default": "",
      "title": "ModifiedBy - Identifies who did a modification(internal)",
      "examples": [
        ""
      ]
    }
  },
  "examples": [{
    "name": "",
    "applicationId": "",
    "serialVersionUID": 0,
    "id": "",
    "createdAt": {
      "MIN_SECOND": 0,
      "MAX_SECOND": 0,
      "serialVersionUID": 0,
      "seconds": 0,
      "nanos": 0
    },
    "createdBy": "",
    "modifiedBy": ""
  }]
}